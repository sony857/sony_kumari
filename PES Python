tup3=(99,12,34,21,55)
if tup1>tup2 and tup1>tup3:
  print( "Tup1 is the biggest")
elif tup2>tup1 and tup2>tup3:
  print ("tup2 is the biggest")
else:
  print ("tup3 is the biggest of all")
